<div class="col-md-4"></div>

	<div class="col-md-4">

<!-- ============================ COMPONENT LOGIN 3  ================================= -->
	<div class="card">
      <div class="card-body">
      <h4 class="card-title mb-4">Sign in</h4>
      <form method="post" action="/admin/login/">
          <div class="form-group">
            <div class="input-group">
				<div class="input-group-prepend">
				    <span class="input-group-text"> <i class="fa fa-envelope"></i> </span>
				 </div>
				<input name="adminEmail" class="form-control" placeholder="Email" type="text">
			</div> <!-- input-group.// -->
          </div> <!-- form-group// -->
          <div class="form-group">
            <div class="input-group">
				<div class="input-group-prepend">
				    <span class="input-group-text"> <i class="fa fa-lock"></i> </span>
				 </div>
				<input name="adminPassword" class="form-control" placeholder="Password" type="password" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;">
			</div> <!-- input-group.// -->
          </div> <!-- form-group// -->

          <div class="form-group">
              <button type="submit" class="btn btn-primary btn-block"> Login  </button>
          </div> <!-- form-group// -->

      </form>
      </div> <!-- card-body.// -->
    </div> <!-- card .// -->
<!-- ============================ COMPONENT LOGIN 3 END.// ================================= -->

	</div>

